    <com.ts.can.RadarUIView android:id="@+id/right_side_radar" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="32px" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
</RelativeLayout>

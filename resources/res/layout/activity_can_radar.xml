    <include layout="@layout/can_right_radar"/>
    <com.ts.can.RadarUIView android:id="@+id/big_left_side_radar" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="170px" android:layout_marginTop="400px"/>
    <com.ts.can.RadarUIView android:id="@+id/big_right_side_radar" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="510px" android:layout_marginTop="400px"/>
